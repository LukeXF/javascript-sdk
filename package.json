{
  "name": "paymentrails",
  "version": "0.9.4",
  "description": "",
  "main": "./dist/index",
  "types": "./dist/index",
  "directories": {
    "test": "tests"
  },
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "test": "npm run test:unit",
    "test:unit": "TS_NODE_PROJECT=test mocha --require ts-node/register \"test/unit/**/*Spec.ts\"",
    "test:integration": "TS_NODE_PROJECT=test mocha --require ts-node/register \"test/integration/**/*Spec.ts\"",
    "build": "rm -rf dist/ && tsc && echo Done",
    "docs": "rm -rf docs && ./node_modules/.bin/typedoc --mode file --name 'Payment Rails JavaScript SDK' --readme none --theme markdown --out docs --hideGenerator lib",
    "clean": "rm -rf dist/"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@types/request": "^2.0.9",
    "request": "^2.81.0"
  },
  "devDependencies": {
    "@types/core-js": "^0.9.44",
    "@types/es6-promise": "^3.3.0",
    "@types/jasmine": "^2.8.4",
    "@types/mocha": "^2.2.47",
    "@types/node": "^9.4.0",
    "@types/sinon": "^4.1.3",
    "@types/uuid": "^3.4.3",
    "mocha": "^5.0.0",
    "sinon": "^4.2.2",
    "ts-node": "^4.1.0",
    "tslint": "^5.9.1",
    "tslint-eslint-rules": "^4.1.1",
    "tslint-microsoft-contrib": "^5.0.2",
    "typedoc": "^0.9.0",
    "typedoc-plugin-markdown": "^1.0.12",
    "typescript": "^2.6.2",
    "uuid": "^3.2.1"
  }
}
